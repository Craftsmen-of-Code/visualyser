{
  "name": "visualyser",
  "displayName": "Visualyser",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "visualyser.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "Visualyser-explorer",
          "title": "Visualyser",
          "icon": "media/graphIcon.svg"
        }
      ]
    },
    "views": {
      "Visualyser-explorer": [
        {
          "id": "mainContent",
          "name": "Visualyser",
          "icon": "media/graphIcon.svg",
          "contextualTitle": "Visualyser"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "mainContent",
        "contents": "No React Project found.\n[Add Project](command:mainContent.addEntry)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}